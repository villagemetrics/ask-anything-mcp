{
  "name": "@villagemetrics-public/ask-anything-mcp",
  "version": "1.0.2",
  "description": "Model Context Protocol server for Village Metrics behavioral tracking data access",
  "type": "module",
  "main": "src/lib/index.js",
  "exports": {
    ".": "./src/lib/index.js",
    "./server": "./src/index.js"
  },
  "bin": {
    "ask-anything-mcp": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "mocha test/**/*.test.js --timeout 30000 --require ./test/setup.cjs",
    "test:manual": "node test/manual-test.js",
    "dev": "VM_LOG_LEVEL=debug node src/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "axios": "^1.7.2",
    "bunyan": "^1.8.15",
    "jsonwebtoken": "^9.0.2",
    "npm-registry-fetch": "^19.0.0",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "chai": "^4.3.10",
    "dotenv": "^17.2.2",
    "mocha": "^10.8.0",
    "sinon": "^21.0.0"
  },
  "keywords": [
    "village metrics mcp",
    "villagemetrics mcp",
    "behavioral tracking mcp",
    "autism care mcp",
    "behavioral data mcp",
    "care coordination mcp",
    "medication tracking mcp"
  ],
  "author": "Doug Kerwin",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
